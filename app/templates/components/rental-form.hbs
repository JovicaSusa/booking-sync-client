<div class='form-group'>
  <label>Name</label>
  {{input
    data-test-rental-name
    value=(mut rental.name)
    class='form-control'}}
</div>
<div class='form-group'>
  <label>Daily Rate</label>
  {{input
    data-test-rental-daily-rate
    type='number'
    value=(mut rental.dailyRate)
    class='form-control'}}
</div>

<div class='text-center'>
  <button data-test-rental-submit class='btn btn-primary' {{action 'handleSubmit' rental}}>
    Save
  </button>
</div>
